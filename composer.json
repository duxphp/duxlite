{
  "name": "duxphp/duxlite",
  "type": "project",
  "description": "Lightweight project development system",
  "keywords": [
    "duxphp",
    "slimphp"
  ],
  "require": {
    "duxphp/lite": "dev-main",
    "xantios/mimey": "^2.2",
    "yansongda/pay": "^3.2",
    "swow/swow": "^1.1",
    "overtrue/easy-sms": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Console\\": "console/"
    }
  },
  "config": {
    "allow-plugins": {
      "duxphp/lite": true
    }
  },
  "repositories": [
    {
      "type" : "composer",
      "url" : "https://packages.duxphp.com"
    }
  ],
  "license": "MIT"
}